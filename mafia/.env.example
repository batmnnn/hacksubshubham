# Private Key for wallet deployment (DO NOT commit real private keys!)
DEPLOYER_PRIVATE_KEY=your_private_key_here

# Monad Testnet Configuration
MONAD_RPC_URL=https://testnet-rpc.monad.xyz
MONAD_CHAIN_ID=10143

# For Scaffold-ETH 2 frontend
NEXT_PUBLIC_ENABLE_TESSERA=true

# Optional: Alchemy keys for other networks (not needed for Monad)
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Optional: Etherscan API keys (not needed for Monad testnet)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: WalletConnect Project ID for enhanced wallet connections
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_walletconnect_project_id_here
